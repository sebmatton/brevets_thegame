<!DOCTYPE html>
<html lang="fr">
<head>
	<meta charset="utf-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
	<title>{% block pagetitle %}{{ attribute(routing,current_page).text }}{% if attribute(routing,current_page).parent is defined and attribute(routing, attribute(routing,current_page).parent) is defined %} - {{ attribute(routing, attribute(routing,current_page).parent).text }}{% endif %}{% endblock %} | {{ layout.site_name }}</title>
	<link rel="stylesheet" href="{{ layout.base_url }}assets/bootstrap/css/bootstrap.min.css" >
    <link href="{{ layout.base_url }}assets/vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <link href="{{ layout.base_url }}assets/vendor/simple-line-icons/simple-line-icons.min.css" rel="stylesheet" type="text/css" />
    <link rel="apple-touch-icon" sizes="57x57" href="{{ layout.base_url }}assets/img/apple-icon-57x57.png">
    <link rel="apple-touch-icon" sizes="60x60" href="{{ layout.base_url }}assets/img/apple-icon-60x60.png">
    <link rel="apple-touch-icon" sizes="72x72" href="{{ layout.base_url }}assets/img/apple-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="76x76" href="{{ layout.base_url }}assets/img/apple-icon-76x76.png">
    <link rel="apple-touch-icon" sizes="114x114" href="{{ layout.base_url }}assets/img/apple-icon-114x114.png">
    <link rel="apple-touch-icon" sizes="120x120" href="{{ layout.base_url }}assets/img/apple-icon-120x120.png">
    <link rel="apple-touch-icon" sizes="144x144" href="{{ layout.base_url }}assets/img/apple-icon-144x144.png">
    <link rel="apple-touch-icon" sizes="152x152" href="{{ layout.base_url }}assets/img/apple-icon-152x152.png">
    <link rel="apple-touch-icon" sizes="180x180" href="{{ layout.base_url }}assets/img/apple-icon-180x180.png">
    <link rel="icon" type="image/png" sizes="192x192"  href="{{ layout.base_url }}assets/img/android-icon-192x192.png">
    <link rel="icon" type="image/png" sizes="32x32" href="{{ layout.base_url }}assets/img/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="96x96" href="{{ layout.base_url }}assets/img/favicon-96x96.png">
    <link rel="icon" type="image/png" sizes="16x16" href="{{ layout.base_url }}assets/img/favicon-16x16.png">
    <link rel="manifest" href="{{ layout.base_url }}assets/img/manifest.json">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="{{ layout.base_url }}assets/img/ms-icon-144x144.png">
    <meta name="theme-color" content="#d64635">
    {% block page_css %}
    {% endblock %}
	<link rel="stylesheet" href="{{ layout.base_url }}assets/css/style.css" >
</head>
<body>
    {% block top %}
    {% endblock %}
    {% include 'layout_flash.twig' %}
    {% block content %}
	{% endblock %}

    {% include 'layout_footer.twig' %}

<script src="{{ layout.base_url }}assets/vendor/js/jquery.min.js" type="text/javascript"></script>
<script src="{{ layout.base_url }}assets/bootstrap/js/bootstrap.bundle.min.js" type="text/javascript"></script>
<script src="{{ layout.base_url }}assets/vendor/js/js.cookie.min.js" type="text/javascript"></script>
<script src="{{ layout.base_url }}assets/vendor/js/jquery-slimscroll/jquery.slimscroll.min.js" type="text/javascript"></script>
{% block page_js %}
{% endblock %}
</body>
</html>